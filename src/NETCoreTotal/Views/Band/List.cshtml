@using MyHobbies;

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Band</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
</head>
<body>
    <h1>List of bands</h1>
    
    <a href="ListWithMembers">List with members</a>

    <ul>
        @foreach (Band band in ViewBag.bands)
        {
            <li>@band.Name (@band.Year)</li>
        }
    </ul>
</body>
</html>